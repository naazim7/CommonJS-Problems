const number=[10,9,9,8,8,11,8,0,9,1]

function getSecondLargest(nums) {




//   first of all we have to remove duplicate item from this arary


const nodupli= nums.filter((value,index,nums)=>{
    return nums.indexOf(value)===index;
})

// then we have to sort this array 

const sorted= nodupli.sort((a,b)=>a-b)


// then we return the 2nd From last

return sorted[sorted.length-2]


}

console.log(getSecondLargest(number))
